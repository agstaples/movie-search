{
  "name": "movie-search",
  "version": "1.0.0",
  "description": "Basic app that allows users to search for movies using TMDB",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm start && nodemon ../server/app.js",
    "client": "webpack-dev-server --mode development --open",
    "server": "nodemon ../server/app.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "postinstall": "(cd ../server && npm install)"
  },
  "author": "Anne G Staples",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^3.4.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^5.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.1",
    "sass-loader": "7.0.1",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.4.1",
    "concurrently": "^5.1.0",
    "history": "^4.10.1",
    "local-storage": "^2.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-number-format": "^4.4.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "redux": "^4.0.5"
  }
}
